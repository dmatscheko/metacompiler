var names = [];
function getNameIdx(name) {
    var pos = names.indexOf(name);
    if (pos != -1) { return pos };
    return names.push(name)-1;
}

var prodsPos = {};
var lastPos = 0
function buildProduction(prodName, prodTag, prodExpression) {
    prodsPos[prodName.Int] = lastPos++
    if (prodTag != undefined) {
        prodTag.Childs = simplifyToArr(prodExpression)
        return abnf.newProduction(prodName.String, prodName.Int, [prodTag], prodName.Pos)
    } else {
        return abnf.newProduction(prodName.String, prodName.Int, simplifyToArr(prodExpression), prodName.Pos)
    }
}

// This breaks up an abnf.oid.Group. Use only for childs of unbreakable rules.
function simplifyArr(rules) {
    if (rules.length == 1) {
        var op = rules[0].Operator
        if ( op == abnf.oid.Sequence || op == abnf.oid.Group || (op == abnf.oid.Or && rules[0].Childs.length <= 1) ) {
            return simplifyArr(rules[0].Childs)
        }
    }
    return rules
}

// This also breaks up an abnf.oid.Group. Use only for childs of unbreakable rules.
function simplifyToArr(rule) {
    return simplifyArr([rule])
}

// Groups with only one child can be broken apart as long as down there is an unbreakable rule. Try to find one.
function trySimplifyDown(rule) {
    if (rule.Childs == undefined) {
        return rule
    }
    var op = rule.Operator
    if (rule.Childs.length == 1) {
        if (op == abnf.oid.Sequence || op == abnf.oid.Group || op == abnf.oid.Or) {
            return trySimplifyDown(rule.Childs[0])
        }
    }
    if (op == abnf.oid.Sequence) {
        return undefined
    } else {
        return rule
    }
}

function simplify(rule) {
    var ruleDown = trySimplifyDown(rule)
    if (ruleDown != undefined) {
        return ruleDown
    }
    if (rule.Childs.length == 1) { // Breaking up abnf.oid.Group did not work. Getting down only with Sequence and Or.
        var op = rule.Operator
        if (op == abnf.oid.Sequence || op == abnf.oid.Or) {
            return simplify(rule.Childs[0])
        }
    }
    return rule
}

function resolveNameIdx(productions) {
	for (var i = 0; i < productions.length; i++) {
        var rule = productions[i]

        if (rule.Childs != undefined && rule.Childs.length > 0) {
            resolveNameIdx(rule.Childs)
        }

		if (rule.Operator == abnf.oid.Production || rule.Operator == abnf.oid.Ident) {
			rule.Int = prodsPos[rule.Int]
		}
	}
}

function getProductions(rules) {
    for (var i = 0; i< rules.length; i++) {
        if (rules[i].Operator == abnf.oid.Sequence) {
            return rules[i].Childs
        }
    }
    return []
}

c.compile(c.asg)
var rules = ltr.stack
var productions = getProductions(rules)
resolveNameIdx(productions)

// To return the generated a-grammar to the next grammarparser:
ltr.agrammar = rules
// To show the initial a-grammar:
print("   => Rules: "+abnf.serializeRules(rules)+"\n\n")
