{

# This is the start rule
ABNF       = [ Title <~~ pushg(pop()) ~~> ] [ Prolog <~~ pushg(pop()) ~~> ] "{" <~~ pushg([]) ~~> { Production <~~ pushg(append(popg(), pop())) //3 ~~> | Comment } "}" <~~ pushg(abnf.newSequence(popg(), c.Pos)) ~~> [ Start <~~ pushg(pop()) ~~> ] [ Description <~~ pushg(pop()) ~~> ] ;

Production  <~~ push(popg()) ~~>
            = Name <~~ pushg(pop()) ~~> [ Tag <~~ ltr.prodTag=pop() ~~> ] "=" [ Expression <~~ pushg(buildProduction(popg(), ltr.prodTag, pop())); ltr.prodTag=undefined ~~> ] ";" ;

Expression  <~~ push(simplify(abnf.newAlternative(popg(), c.Pos))) ~~>
            = Alternative <~~ pushg([pop()]) ~~> { "|" Alternative <~~ pushg(append(popg(), pop())) //2 ~~> } ;

Alternative <~~ push(simplify(abnf.newSequence(popg(), c.Pos))) ~~>
            = Term <~~ pushg([pop()]) ~~> { Term <~~ pushg(append(popg(), pop())) //1 ~~> } ;

Term        = Range | TaggedTerm ;

TaggedTerm <~~ push(popg()) ~~>
            = ( Name | Group | Option | Repetition | Skipspaces ) <~~ pushg(simplify(pop())) ~~> [ Tag <~~ var tag=pop(); tag.Childs=simplifyToArr(popg()); pushg(tag) ~~> ] ;

Range       <~~ push(popg()) ~~>
            = TaggedToken <~~ pushg(pop()) ~~> [ "..." TaggedToken <~~ pushg(abnf.newRange([popg(), pop()], c.Pos)) ~~> ] ;
Group       = "(" Expression <~~ push(abnf.newGroup(simplifyToArr(pop()), c.Pos)) ~~> ")" ;
Option      = "[" Expression <~~ push(abnf.newOption(simplifyToArr(pop()), c.Pos)) ~~> "]" ;
Repetition  = "{" Expression <~~ push(abnf.newRepetition(simplifyToArr(pop()), c.Pos)) ~~> "}" ;
Skipspaces  = Skip | Noskip ;

TaggedToken <~~ push(popg()) ~~>
            = Token <~~ pushg(pop()) ~~> [ Tag <~~ var tag=pop(); tag.Childs=simplifyToArr(popg()); pushg(tag) ~~> ] ;

Start       = Name ;
Title       = Token ;
Description = Token ;
Comment     = "#" - { AsciiNoLb } ( "\n" | "\r" ) + ;

Prolog      = Tag ;

Tag         <~~ push(abnf.newTag(popg(), undefined, c.Pos)) ~~>
            = "<" Code <~~ pushg([pop()]) ~~> { "," Code <~~ pushg(append(popg(), pop())) ~~> } ">" ;

Code        = '~~' - { [ "~" ] AllButTilde } <~~ push(abnf.newToken(up.in, c.Pos)) ~~> '~~' + ;

Name        <~~ push(abnf.newName(up.in, getNameIdx(up.in), c.Pos)) ~~>
            = Alphabet - { Alphabet | Digit | "_" } + ;
Token       = Dquotetoken | Squotetoken ;

Dquotetoken = '"' - { AsciiNoQs | "'" | '\\"' } <~~ push(abnf.newToken(unescape(up.in), c.Pos)) ~~> '"' + ;
Squotetoken = "'" - { AsciiNoQs | '"' | "\\'" } <~~ push(abnf.newToken(unescape(up.in), c.Pos)) ~~> "'" + ;

Alphabet    = "a"..."z" | "A"..."Z" ;
Digit       = "0"..."9" ;
AsciiNoQs   = "\x28"..."\x7E" | "\x23"..."\x26" | "\t" | "\n" | "\r" | " " | "!" ; # Readable ASCII without double and single quotes
AsciiNoLb   = "\x20"..."\x7E" | "\t" ; # Readable ASCII withput line breaks (CR and LF)
AllButTilde = "\x00"..."\x7D" | "\\~" | "\x7f"..."\uffff" ; # All ASCII and unicode chars. Only tilde is escaped

Skip        = "+" <~~ push(abnf.newSkipSpace(true, c.Pos)) ~~> ;
Noskip      = "-" <~~ push(abnf.newSkipSpace(false, c.Pos)) ~~> ;

}
ABNF
